{
  "expo": {
    "name": "MobileApp",
    "slug": "mobileapp",
    "owner": "omar-salah-elboredy",
    "privacy": "hidden",
    "version": "1.0.17072023",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ce3c7ad7-529c-40ca-9877-dc30206a1cd6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abdulazizmassoud.MobileApp",
      "buildNumber": "1.0.15072023",
      "usesIcloudStorage": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "mail.app.v1"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ce3c7ad7-529c-40ca-9877-dc30206a1cd6"
      }
    },
    "runtimeVersion": "exposdk:47.0.0",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "sentry-expo"
    ],
    "build": {
      "preview": {
        "distribution": "internal"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "neworganisation",
            "project": "mail-app",
            "authToken": false
          }
        }
      ]
    }
  }
}
